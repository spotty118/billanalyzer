                                    <script>
                                        ///////// header-ACCORDION FUNCTIONALITY /////////
                                        var acc = document.getElementsByClassName("header-accordion-header");
                                        var i;
                                        // ADDS EVENT LISTENER TO EACH ACCORDION
                                        for (i = 0; i < acc.length; i++) {
                                            acc[i].addEventListener("click", function() {
                                                var panel = this.nextElementSibling;
                                                var icon = this.querySelector(".header-accordion-icon");
                                                // IF ACCORDION IS ACTIVE, INACTIVATE IT, ELSE ACTIVATE IT AND RESET ALL OTHERS IN SECTION
                                                if (panel.classList.contains("active-accordion")) {
                                                    panel.classList.remove("active-accordion");
                                                    panel.style.maxHeight = null;
                                                    panel.style.paddingTop = 0;
                                                    panel.style.paddingBottom = 0;
                                                    panel.style.borderTop = null;
                                                    icon.style.transform = null;
                                                    panel.style.overflow = "hidden";
                                                } else {
                                                    var activeAccordions = document.querySelectorAll(".active-accordion");
                                                    var activeIcons = document.querySelectorAll(".header-accordion-icon");
                                                    if (activeAccordions.length > 0) {
                                                        activeAccordions.forEach(function(el) {
                                                            el.classList.remove("active-accordion");
                                                            el.style.maxHeight = null;
                                                            el.style.paddingTop = 0;
                                                            el.style.paddingBottom = 0;
                                                            el.style.borderTop = null;
                                                            el.style.overflow = "hidden";
                                                        });
                                                        activeIcons.forEach(function(iconEl) {
                                                            iconEl.style.transform = null;
                                                        })
                                                    }
                                                    panel.classList.add("active-accordion");
                                                    panelHeight = panel.scrollHeight + 50;
                                                    panel.style.maxHeight = panelHeight + "px";
                                                    panel.style.paddingTop = "24px";
                                                    panel.style.paddingBottom = "24px";
                                                    panel.style.borderTop = "1px solid #cccccc";
                                                    icon.style.transform = "rotate(45deg)";
                                                    panel.style.overflow = null;
                                                }
                                            });
                                        }
                                    </script>
                                </div>
                            </div>
