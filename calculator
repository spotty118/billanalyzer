                            <div class="elementor-element elementor-element-ef165c2 elementor-widget elementor-widget-html" data-id="ef165c2" data-element_type="widget" data-widget_type="html.default">
                                <div class="elementor-widget-container">
                                    <script rel="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.2/jquery.validate.min.js"></script>
                                    <script type="text/javascript">
                                        jQuery(document).ready(function($) {
                                            let isSubmit = false;
                                            var plans = {
                                                welcome: 'Unlimited Welcome',
                                                plus: 'Unlimited Plus',
                                                unlimited: "Unlimited"
                                            }
                                            var prices = {
                                                welcome_1: 65,
                                                welcome_2: 55,
                                                welcome_3: 40,
                                                welcome_4: 30,
                                                welcome_5: 27,
                                                plus_1: 80,
                                                plus_2: 70,
                                                plus_3: 55,
                                                plus_4: 45,
                                                plus_5: 42,
                                                unlimited_1: 90,
                                                unlimited_2: 80,
                                                unlimited_3: 65,
                                                unlimited_4: 55,
                                                unlimited_5: 52
                                            }
                                            $("#calcForm").validate({
                                                rules: {
                                                    phone_bill: {
                                                        required: true,
                                                        number: true
                                                    },
                                                    streaming_bill: {
                                                        required: true,
                                                        number: true
                                                    },
                                                    plan: {
                                                        required: true
                                                    },
                                                    lines: {
                                                        required: true
                                                    }
                                                },
                                                groups: {
                                                    groupFields: "phone_bill streaming_bill plan lines"
                                                },
                                                messages: {
                                                    phone_bill: {
                                                        required: "Complete all fields."
                                                    },
                                                    streaming_services: {
                                                        required: "Complete all fields."
                                                    },
                                                    plan: {
                                                        required: "Complete all fields."
                                                    },
                                                    lines: {
                                                        required: "Complete all fields."
                                                    }
                                                },
                                                errorElement: 'div',
                                                errorLabelContainer: '.errorTxt',
                                                submitHandler: function(form) {
                                                    isSubmit = true;
                                                    // Hide the temp div and then show the calculator
                                                    $('#calc-no').hide();
                                                    $('#calc-yes').show();
                                                    /*********************
                                                     * Set Form Fields *
                                                     *********************/
                                                    // Set the phone bill value
                                                    var phoneBill = $("#phone_bill").val();
                                                    // Set the number of lines
                                                    var planLines = $("#lines").val();
                                                    // Set the streaming services value
                                                    var streamingBill = $("#streaming_bill").val();
                                                    // Set the plan name
                                                    var plan = $("#plan").val();
                                                    var planName = plans[plan];
                                                    /*********************
                                                     * Current Column *
                                                     *********************/
                                                    // Set Plan Bill
                                                    var currentBillByLine = phoneBill / planLines;
                                                    currentBillByLine = currentBillByLine.toFixed(0);
                                                    document.getElementById('current_plan_bill').innerHTML = '$' + currentBillByLine + '  < span style = "font-size: 11px;" > per month / $ ' + phoneBill + '
                                                    total < /span>';
                                                    // Set lines
                                                    document.getElementById('current_lines').innerHTML = planLines;
                                                    // Set Streaming Bill
                                                    document.getElementById('current_streaming').innerHTML = '$' + streamingBill;
                                                    /*********************
                                                     * New Column *
                                                     *********************/
                                                    // Set Plan Name
                                                    document.getElementById('calc_plan_name').innerHTML = planName;
                                                    // Set Plan Bill
                                                    if (planLines > 4) {
                                                        getPlanLinePrice = 5;
                                                    } else {
                                                        getPlanLinePrice = planLines;
                                                    }
                                                    var price = plan + "_" + getPlanLinePrice;
                                                    var planPrice = "$" + prices[price];
                                                    var newPhoneBill = planLines * prices[price];
                                                    document.getElementById('calc_plan_bill').innerHTML = planPrice;
                                                    document.getElementById('calc_plan_bill').innerHTML = planPrice + '  < span style = "font-size: 11px;" > per month / $ ' + newPhoneBill + '
                                                    total < /span>';
                                                    // Set lines
                                                    document.getElementById('calc_lines').innerHTML = planLines;
                                                    // Set Perk Details
                                                    var perkVal = 0;
                                                    if (document.getElementById('netflix').checked) {
                                                        perkVal += 10;
                                                    };
                                                    if (document.getElementById('disney').checked) {
                                                        perkVal += 10;
                                                    };
                                                    if (document.getElementById('hotspot').checked) {
                                                        perkVal += 10;
                                                    };
                                                    if (document.getElementById('apple_music').checked) {
                                                        perkVal += 10;
                                                    };
                                                    if (document.getElementById('apple_one').checked) {
                                                        perkVal += 10;
                                                    };
                                                    if (document.getElementById('travel').checked) {
                                                        perkVal += 10;
                                                    };
                                                    if (document.getElementById('cloud').checked) {
                                                        perkVal += 10;
                                                    };
                                                    if (document.getElementById('google').checked) {
                                                        perkVal += 10;
                                                    };
                                                    if (document.getElementById('youtube').checked) {
                                                        perkVal += 10;
                                                    };
                                                    var planPerk = "$" + perkVal;
                                                    document.getElementById('calc_perk').innerHTML = planPerk;
                                                    /*********************
                                                     * Savings Column *
                                                     *********************/
                                                    let zeroSavings = document.querySelector(".zero-savings");
                                                    let normSavings = document.querySelector(".norm-savings");
                                                    // Set Savings 
                                                    var currentTotalBill = +streamingBill + +phoneBill;
                                                    var savings = (+streamingBill + +phoneBill) - (+newPhoneBill + +perkVal)
                                                    console.log(currentTotalBill);
                                                    // We don't want to show a negative number
                                                    if (savings > 0) {
                                                        zeroSavings.style.display = "none";
                                                        normSavings.style.display = "block";
                                                        planSavings = "$" + savings;
                                                    } else {
                                                        zeroSavings.style.display = "block";
                                                        normSavings.style.display = "none";
                                                        planSavings = "$0"
                                                    }
                                                    document.getElementById('calc_savings').innerHTML = planSavings;
                                                }
                                            });
                                        });
                                    </script>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
