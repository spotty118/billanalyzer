                                            <div class="elementor-element elementor-element-a270f8e elementor-hidden-desktop elementor-hidden-tablet elementor-hidden-mobile elementor-widget elementor-widget-html" data-id="a270f8e" data-element_type="widget" data-widget_type="html.default">
                                                <div class="elementor-widget-container">
                                                    <script rel="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.2/jquery.validate.min.js"></script>
                                                    <script type="text/javascript">
                                                        jQuery(document).ready(function($) {
                                                            let isSubmit = false;
                                                            var plansMobile = {
                                                                unlimited: "Unlimited Ultimate",
                                                                welcome: 'Unlimited Welcome',
                                                                plus: 'Unlimited Plus'
                                                            }
                                                            var pricesMobile = {
                                                                welcome_1: 65,
                                                                welcome_2: 55,
                                                                welcome_3: 40,
                                                                welcome_4: 30,
                                                                welcome_5: 27,
                                                                plus_1: 80,
                                                                plus_2: 70,
                                                                plus_3: 55,
                                                                plus_4: 45,
                                                                plus_5: 42,
                                                                unlimited_1: 90,
                                                                unlimited_2: 80,
                                                                unlimited_3: 65,
                                                                unlimited_4: 55,
                                                                unlimited_5: 52
                                                            }
                                                            $("#calcFormMobile").validate({
                                                                rules: {
                                                                    phone_bill_mobile: {
                                                                        required: true,
                                                                        number: true
                                                                    },
                                                                    streaming_bill_mobile: {
                                                                        required: true,
                                                                        number: true
                                                                    },
                                                                    plan_mobile_mobile: {
                                                                        required: true
                                                                    },
                                                                    lines_mobile: {
                                                                        required: true
                                                                    }
                                                                },
                                                                groups: {
                                                                    groupFields: "phone_bill_mobile streaming_bill_mobile plan_mobile lines_mobile"
                                                                },
                                                                messages: {
                                                                    phone_bill_mobile: {
                                                                        required: "Complete all fields."
                                                                    },
                                                                    streaming_services_mobile: {
                                                                        required: "Complete all fields."
                                                                    },
                                                                    plan_mobile: {
                                                                        required: "Complete all fields."
                                                                    },
                                                                    lines_mobile: {
                                                                        required: "Complete all fields."
                                                                    }
                                                                },
                                                                errorElement: 'div',
                                                                errorLabelContainer: '.errorTxtMobile',
                                                                submitHandler: function(form) {
                                                                    isSubmit = true;
                                                                    // Hide the temp div and then show the calculator
                                                                    $('#calc-no-mobile').hide();
                                                                    $('#calc-yes-mobile').show();
                                                                    /*********************
                                                                     * Set Form Fields *
                                                                     *********************/
                                                                    // Set the phone bill value
                                                                    var phoneBillMobile = $("#phone_bill_mobile").val();
                                                                    // Set the number of lines
                                                                    var planLinesMobile = $("#lines_mobile").val();
                                                                    // Set the streaming services value
                                                                    var streamingBillMobile = $("#streaming_bill_mobile").val();
                                                                    // Set the plan name
                                                                    var planMobile = $("#plan_mobile").val();
                                                                    var planNameMobile = plansMobile[planMobile];
                                                                    /*********************
                                                                     * Current Column *
                                                                     *********************/
                                                                    // Set Plan Bill
                                                                    var currentBillByLineMobile = phoneBillMobile / planLinesMobile;
                                                                    currentBillByLineMobile = currentBillByLineMobile.toFixed(0);
                                                                    document.getElementById('current_plan_bill_mobile').innerHTML = '$' + currentBillByLineMobile + '  < span style = "font-size: 11px;" > per month / $ ' + phoneBillMobile + '
                                                                    total < /span>';
                                                                    // Set lines
                                                                    document.getElementById('current_lines_mobile').innerHTML = planLinesMobile;
                                                                    // Set Streaming Bill
                                                                    document.getElementById('current_streaming_mobile').innerHTML = '$' + streamingBillMobile;
                                                                    /*********************
                                                                     * New Column *
                                                                     *********************/
                                                                    // Set Plan Name
                                                                    document.getElementById('calc_plan_name_mobile').innerHTML = planNameMobile;
                                                                    // Set Plan Bill
                                                                    if (planLinesMobile > 4) {
                                                                        getPlanLinePrice = 5;
                                                                    } else {
                                                                        getPlanLinePrice = planLinesMobile;
                                                                    }
                                                                    var priceMobile = planMobile + "_" + getPlanLinePrice;
                                                                    var planPriceMobile = "$" + pricesMobile[priceMobile];
                                                                    var newPhoneBillMobile = planLinesMobile * pricesMobile[priceMobile];
                                                                    document.getElementById('calc_plan_bill_mobile').innerHTML = planPriceMobile;
                                                                    document.getElementById('calc_plan_bill_mobile').innerHTML = planPriceMobile + '  < span style = "font-size: 11px;" > per month / $ ' + newPhoneBillMobile + '
                                                                    total < /span>';
                                                                    // Set lines
                                                                    document.getElementById('calc_lines_mobile').innerHTML = planLinesMobile;
                                                                    // Set Perk Details
                                                                    var perkValMobile = 0;
                                                                    if (document.getElementById('netflix_mobile').checked) {
                                                                        perkValMobile += 10;
                                                                    };
                                                                    if (document.getElementById('disney_mobile').checked) {
                                                                        perkValMobile += 10;
                                                                    };
                                                                    if (document.getElementById('hotspot_mobile').checked) {
                                                                        perkValMobile += 10;
                                                                    };
                                                                    if (document.getElementById('apple_music_mobile').checked) {
                                                                        perkValMobile += 10;
                                                                    };
                                                                    if (document.getElementById('apple_one_mobile').checked) {
                                                                        perkValMobile += 10;
                                                                    };
                                                                    if (document.getElementById('travel_mobile').checked) {
                                                                        perkValMobile += 10;
                                                                    };
                                                                    if (document.getElementById('cloud_mobile').checked) {
                                                                        perkValMobile += 10;
                                                                    };
                                                                    if (document.getElementById('google_mobile').checked) {
                                                                        perkValMobile += 10;
                                                                    };
                                                                    if (document.getElementById('youtube_mobile').checked) {
                                                                        perkValMobile += 10;
                                                                    };
                                                                    var planPerkMobile = "$" + perkValMobile;
                                                                    document.getElementById('calc_perk_mobile').innerHTML = planPerkMobile;
                                                                    /*********************
                                                                     * Savings Column *
                                                                     *********************/
                                                                    let zeroSavingsMobile = document.querySelector(".zero-savings-mobile");
                                                                    let normSavingsMobile = document.querySelector(".norm-savings-mobile");
                                                                    // Set Savings 
                                                                    var savingsMobile = (+streamingBillMobile + +phoneBillMobile) - (+newPhoneBillMobile + +perkValMobile);
                                                                    // We don't want to show a negative number
                                                                    if (savingsMobile > 0) {
                                                                        zeroSavingsMobile.style.display = "none";
                                                                        normSavingsMobile.style.display = "block";
                                                                        planSavingsMobile = "$" + savingsMobile;
                                                                    } else {
                                                                        zeroSavingsMobile.style.display = "block";
                                                                        normSavingsMobile.style.display = "none";
                                                                        planSavingsMobile = "$0"
                                                                    }
                                                                    document.getElementById('calc_savings_mobile').innerHTML = planSavingsMobile;
                                                                }
                                                            });
                                                        });
                                                    </script>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div>
                    </div>
                </div>
            </section>
        </div>
